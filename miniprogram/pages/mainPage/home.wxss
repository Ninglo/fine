.background {
  width: 100%;
}
.circle {
  width: 100px;
  height: 100px;
  border: 0 solid #ffffff;
  border-radius: 500px;
  box-shadow: 4px 1px 1px #cccccc;
  color: white;
  font-size: 50rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

button[type='default'] {
  color: white;
  background-color: #ff9900;
}
